package anylayout.util;

import fpeas.function.Function;

import java.util.Map;

public class MapUtility
{
	public static <K,V> V get(final Map<K,V> map,final K key,final Function<K,V> defaultValue)
	{
		final V value=map.get(key);

		return null==value ? defaultValue.run(key) : value;
	}
}